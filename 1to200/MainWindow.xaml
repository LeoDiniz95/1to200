<Window x:Class="_1to200.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_1to200"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="btnCalc" HorizontalAlignment="Center" VerticalAlignment="Center">
        <TextBox x:Name="txtNumbers" HorizontalAlignment="Center" VerticalAlignment="Center" Width="600" Height="100" IsEnabled="False" TextWrapping="Wrap" />
        <Button x:Name="btnAction" Width="200" Margin="200,117,200,-68" Content="Clique Aqui" Click="btnAction_Click" />
        <Label x:Name="lblTitle" Content="1 até 200" HorizontalAlignment="Left" Margin="200,-110,0,0" VerticalAlignment="Top" FontSize="48" FontWeight="Bold" FontFamily="Segoe UI Symbol" IsEnabled="False"/>
    </Grid>
</Window>
